  <guiMetaInformation xsi:type="typhonDL:Import" relativePath="userdb.tdl"/>
  <elements xsi:type="typhonDL:PlatformType" name="default"/>
  <elements xsi:type="typhonDL:ContainerType" name="Docker"/>
  <elements xsi:type="typhonDL:DBType" name="mariadb"/>
  <elements xsi:type="typhonDL:DBType" name="mongo"/>
  <elements xsi:type="typhonDL:Deployment" name="platformname" type="//@elements.0">
    <clusters name="MyCluster">
      <applications name="MyApplication">
        <containers name="container1" type="//@elements.1">
          <deploys>
            <reference href="platform:/resource/example.typhondl/productdb.tdl#//@elements.0"/>
