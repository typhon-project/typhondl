        </containers>
        <containers name="container2" type="//@elements.1">
          <deploys>
            <reference href="platform:/resource/example.typhondl/orderdb.tdl#//@elements.0"/>
          </deploys>
          <properties xsi:type="typhonDL:Key_Value" name="ports" value="8080:2939"/>
          <properties xsi:type="typhonDL:Key_Value" name="testProp1" value="value1"/>
          <properties xsi:type="typhonDL:Key_Value" name="testProp2" value="value2"/>
          <properties xsi:type="typhonDL:Key_Value" name="testProp3" value="value3"/>
        </containers>
        <containers name="container3" type="//@elements.1">
          <deploys>
            <reference href="platform:/resource/example.typhondl/userdb.tdl#//@elements.0"/>
          </deploys>
          <properties xsi:type="typhonDL:Key_Value" name="ports" value="8080:0980398"/>
        </containers>
      </applications>
    </clusters>
  </elements>
</typhonDL:DeploymentModel>
