<?xml version="1.0" encoding="ASCII"?>
<typhonml:Model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:typhonml="http://org.typhon.dsls.typhonml.sirius">
  <databases xsi:type="typhonml:RelationalDB" name="AppData">
    <tables name="AppDB" entity="//@dataTypes.9">
      <indexSpec name="appIndex" attributes="//@dataTypes.9/@attributes.1"/>
    </tables>
    <tables name="WarningDB" entity="//@dataTypes.10">
      <indexSpec name="warningIndex" attributes="//@dataTypes.10/@attributes.1"/>
    </tables>
  </databases>
  <databases xsi:type="typhonml:DocumentDB" name="TextWarningData">
    <collections name="TextWarningDB" entity="//@dataTypes.11"/>
  </databases>
  <dataTypes xsi:type="typhonml:PrimitiveDataType" name="Date"/>
  <dataTypes xsi:type="typhonml:PrimitiveDataType" name="String"/>
  <dataTypes xsi:type="typhonml:PrimitiveDataType" name="double"/>
  <dataTypes xsi:type="typhonml:PrimitiveDataType" name="Color"/>
  <dataTypes xsi:type="typhonml:PrimitiveDataType" name="int"/>
  <dataTypes xsi:type="typhonml:PrimitiveDataType" name="Blob"/>
  <dataTypes xsi:type="typhonml:CustomDataType" name="position">
    <elements name="longitude" type="//@dataTypes.2">
      <implementation location="it.univaq.disim.double"/>
    </elements>
    <elements name="latitute" type="//@dataTypes.2">
      <implementation location="it.univaq.disim.double"/>
    </elements>
  </dataTypes>
  <dataTypes xsi:type="typhonml:CustomDataType" name="timeRange">
    <elements name="begin" type="//@dataTypes.0">
      <implementation location="it.univaq.disim.Date"/>
    </elements>
    <elements name="end" type="//@dataTypes.0">
      <implementation location="it.univaq.disim.Date"/>
    </elements>
  </dataTypes>
  <dataTypes xsi:type="typhonml:CustomDataType" name="polygon">
    <elements name="position_one" type="//@dataTypes.6">
      <implementation location="this.position"/>
    </elements>
    <elements name="position_two" type="//@dataTypes.6">
      <implementation location="this.position"/>
    </elements>
  </dataTypes>
  <dataTypes xsi:type="typhonml:Entity" name="App">
    <attributes name="timeStamp" type="//@dataTypes.0"/>
    <attributes name="vehicle_id" type="//@dataTypes.1"/>
    <attributes name="vehicle_position" type="//@dataTypes.6"/>
    <relations name="warnings" type="//@dataTypes.10" cardinality="zero_many" isContainment="true"/>
  </dataTypes>
  <dataTypes xsi:type="typhonml:Entity" name="Warning">
    <attributes name="time" type="//@dataTypes.7"/>
    <attributes name="id" type="//@dataTypes.1"/>
    <attributes name="warningType" type="//@dataTypes.1"/>
    <attributes name="severity" type="//@dataTypes.4"/>
    <attributes name="area" type="//@dataTypes.8"/>
  </dataTypes>
  <dataTypes xsi:type="typhonml:Entity" name="TextWarning">
    <attributes name="timeStamp" type="//@dataTypes.0"/>
    <attributes name="id" type="//@dataTypes.1"/>
    <attributes name="text" type="//@dataTypes.5"/>
    <fretextAttributes name="Meteosafe">
      <tasks type="RelationExtraction"/>
      <tasks type="TermExtraction"/>
    </fretextAttributes>
  </dataTypes>
</typhonml:Model>
