datatype Date
datatype String
datatype Double
datatype int

entity App {
	timeStamp : Date
	vehicle_id : String
	vehicle_position : Double
	warnings :-> Warning[0..*]
	textwarnings :-> TextWarning[0..*]
}

entity Warning {
	time : Date
	warning_id : String
	warningType : String
	severity : int
	area : Double
}

entity TextWarning {
	timeStamp : Date
	textWarning_id : String
	text : String
	freetext Meteosafe [RelationExtraction, TermExtraction]
}

documentdb TextWarningData {
	collections {
		TextWarningDB : TextWarning
		AppDB : App
		WarningDB : Warning
	}
}

