<?xml version="1.0" encoding="UTF-8"?>
<typhonDL:DeploymentModel xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:typhonDL="http://www.typhon.org/dsls/xtext/TyphonDL">
  <guiMetaInformation xsi:type="typhonDL:Import" relativePath="weather_warning_ML.xmi"/>
  <guiMetaInformation xsi:type="typhonDL:Import" relativePath="TextWarningData.tdl"/>
  <guiMetaInformation xsi:type="typhonDL:Import" relativePath="AppData.tdl"/>
  <guiMetaInformation xsi:type="typhonDL:Import" relativePath="dbTypes.tdl"/>
  <elements xsi:type="typhonDL:PlatformType" name="local"/>
  <elements xsi:type="typhonDL:ContainerType" name="Docker"/>
  <elements xsi:type="typhonDL:ClusterType" name="DockerCompose"/>
  <elements xsi:type="typhonDL:Platform" name="platformName" type="//@elements.0">
    <clusters name="clusterName" type="//@elements.2">
      <applications name="Polystore">
        <containers name="TextWarningData" type="//@elements.1">
          <deploys reference="//@elements.4"/>
          <properties xsi:type="typhonDL:Key_KeyValueList" name="ports">
            <key_Values name="containerPort" value="27017"/>
            <key_Values name="publishedPort" value="27018"/>
          </properties>
        </containers>
        <containers name="AppData" type="//@elements.1">
          <deploys reference="//@elements.5"/>
          <properties xsi:type="typhonDL:Key_KeyValueList" name="ports">
            <key_Values name="containerPort" value="3306"/>
            <key_Values name="publishedPort" value="3306"/>
          </properties>
        </containers>
        <containers name="polystore-mongo" type="//@elements.1">
          <deploys reference="//@elements.8"/>
          <properties xsi:type="typhonDL:Key_Values" name="hostname" value="polystore-mongo"/>
          <properties xsi:type="typhonDL:Key_KeyValueList" name="ports">
            <key_Values name="publishedPort" value="27017"/>
            <key_Values name="containerPort" value="27017"/>
          </properties>
        </containers>
        <containers name="typhon-polystore-service" type="//@elements.1">
          <deploys reference="//@elements.9"/>
          <depends_on reference="//@elements.3/@clusters.0/@applications.0/@containers.2"/>
          <properties xsi:type="typhonDL:Key_Values" name="restart" value="always"/>
          <properties xsi:type="typhonDL:Key_Values" name="hostname" value="polystore-api"/>
          <properties xsi:type="typhonDL:Key_KeyValueList" name="ports">
            <key_Values name="publishedPort"/>
            <key_Values name="containerPort"/>
          </properties>
        </containers>
        <containers name="polystore-ui" type="//@elements.1">
          <deploys reference="//@elements.10"/>
          <depends_on reference="//@elements.3/@clusters.0/@applications.0/@containers.3"/>
          <properties xsi:type="typhonDL:Key_KeyValueList" name="ports">
            <key_Values name="publishedPort" value="4200"/>
            <key_Values name="containerPort" value="4200"/>
          </properties>
          <properties xsi:type="typhonDL:Key_Values" name="restart" value="always"/>
          <properties xsi:type="typhonDL:Key_Values" name="hostname" value="polystore-ui"/>
        </containers>
      </applications>
    </clusters>
  </elements>
  <elements xsi:type="typhonDL:DB" name="TextWarningData" type="//@elements.6">
        <key_Values name="API_HOST" value="localhost"/>
    <parameters xsi:type="typhonDL:Key_KeyValueList" name="environment">
      <key_Values name="MONGO_INITDB_ROOT_USERNAME" value="admin"/>
      <key_Values name="MONGO_INITDB_ROOT_PASSWORD" value="admin"/>
    </parameters>
  </elements>
  <elements xsi:type="typhonDL:DB" name="AppData" type="//@elements.7"/>
  <elements xsi:type="typhonDL:DBType" name="mongo">
    <image value="mongo:latest"/>
  </elements>
  <elements xsi:type="typhonDL:DBType" name="mariadb">
    <image value="mariadb:latest"/>
  </elements>
  <elements xsi:type="typhonDL:DB" name="polystore_db" type="//@elements.6">
    <parameters xsi:type="typhonDL:Key_KeyValueList" name="environment">
      <key_Values name="MONGO_INITDB_ROOT_USERNAME" value="admin"/>
      <key_Values name="MONGO_INITDB_ROOT_PASSWORD" value="admin"/>
    </parameters>
  </elements>
  <elements xsi:type="typhonDL:Software" name="polystore_api">
    <image value="clms/typhon-polystore-api:latest"/>
  </elements>
  <elements xsi:type="typhonDL:Software" name="polystore_ui">
    <image value="clms/typhon-polystore-ui:latest"/>
    <parameters xsi:type="typhonDL:Key_KeyValueList" name="environment">
      <key_Values name="API_PORT" value="8080"/>
      <key_Values name="API_HOST" value="localhost"/>
    </parameters>
  </elements>
</typhonDL:DeploymentModel>
