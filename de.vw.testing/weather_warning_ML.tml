datatype Date
datatype String
datatype int

entity App {
	timeStamp : Date
	vehicle_id : String
	vehicle_position : int
	warnings :-> Warning[0..*]
	textwarnings :-> TextWarning[0..*]
}

entity Warning {
	time : Date
	warning_id : String
	warningType : String
	severity : int
	area : int
}

entity TextWarning {
	timeStamp : Date
	textWarning_id : String
	text : String
}


relationaldb AppData {
	tables {
		table {
			AppDB : App 
			index appIndex {
				attributes ("App.vehicle_id")
			}
		}
		table {
			WarningDB : Warning
			index warningIndex {
				attributes ("Warning.warning_id")
			}
		}
	}
}

documentdb TextWarningData {
	collections {
		TextWarningDB : TextWarning
	}
}