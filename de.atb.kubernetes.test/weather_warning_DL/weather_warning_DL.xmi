          <deploys reference="//@elements.4"/>
          <ports>
            <key_values name="target" value="27017"/>
            <key_values name="published" value="31074"/>
          </ports>
          <properties xsi:type="typhonDL:Key_Values" name="hostname" value="&quot;192.168.99.101&quot;"/>
        </containers>
        <containers name="AppData" type="//@elements.0">
          <deploys reference="//@elements.5"/>
          <ports>
            <key_values name="target" value="3306"/>
            <key_values name="published" value="31090"/>
          </ports>
          <properties xsi:type="typhonDL:Key_Values" name="hostname" value="&quot;192.168.99.101&quot;"/>
        </containers>
        <containers name="polystore-mongo" type="//@elements.0">
          <deploys reference="//@elements.8"/>
          <ports>
            <key_values name="published" value="30064"/>
            <key_values name="target" value="27017"/>
          </ports>
          <properties xsi:type="typhonDL:Key_Values" name="hostname" value="192.168.99.101"/>
        </containers>
        <containers name="typhon-polystore-service" type="//@elements.0">
          <deploys reference="//@elements.9"/>
