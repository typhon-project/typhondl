<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<templates>
<template autoinsert="true" type="relationaldb" context="de.atb.typhondl.xtext.TyphonDL.DB" deleted="false" description="basic template for MariaDB" enabled="true" name="MariaDB" id="1">
database ${databaseName} : mariadb {
	environment {
		MYSQL_ROOT_PASSWORD = example ;
		}
	}	
</template>
<template autoinsert="true" type="documentdb" context="de.atb.typhondl.xtext.TyphonDL.DB" deleted="false" description="basic template for Mongo" enabled="true" name="Mongo" id="2">
database ${databaseName} : mongo {
	environment {
		MONGO_INITDB_ROOT_USERNAME = admin ;
		MONGO_INITDB_ROOT_PASSWORD = admin ;
		}
	}	
</template>
<template autoinsert="true" type="relationaldb" context="de.atb.typhondl.xtext.TyphonDL.DB" deleted="false" description="basic template for MySQL" enabled="false" name="MySQL" id="3">
database ${databaseName} : mysql {
	environment {
		MYSQL_ROOT_PASSWORD = example ;
		}
	}	
</template>
<template autoinsert="true" context="de.atb.typhondl.xtext.TyphonDL.DBType" deleted="false" description="basic template for DBType" enabled="true" name="DBType" id="4">
dbtype ${dbms} {
	default image = ${dbms}:latest ;
}
</template>
</templates>